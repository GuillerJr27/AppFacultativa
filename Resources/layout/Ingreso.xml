<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:p4="http://xamarin.com/mono/android/designer"
    android:orientation="vertical"
    android:padding="10.0dp"
    android:background="@color/background8"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tableRow1">
        <ImageView
            android:contentDescription=""
            android:layout_width="37.0dp"
            android:layout_height="30.5dp"
            android:src="@drawable/ingresos"
            android:id="@+id/imageView1"
            android:layout_marginBottom="14.5dp" />
        <TextView
            android:layout_gravity="center"
            android:textAlignment="center"
            android:text="@string/textView26"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="253.5dp"
            android:layout_height="40.5dp"
            android:minWidth="25px"
            android:minHeight="25px"
            android:textColor="#000000"
            android:id="@+id/textView3"
            android:layout_marginLeft="15.5dp" />
        
    </TableRow>
    <ImageView
        android:src="@drawable/login"
        android:layout_width="353.5dp"
        android:layout_height="125.0dp"
        android:contentDescription=""
        android:id="@+id/imageView3"
        android:layout_marginRight="0.0dp" />
    
    <ScrollView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scrollView2">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/tableLayout1">
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:id="@+id/tableRow2">
                <ImageView
                    android:contentDescription=""
                    android:layout_width="37.0dp"
                    android:layout_height="40dp"
                    android:src="@drawable/nombrecuenta"
                    android:id="@+id/imageView4" />

                <EditText
                    android:hint="@string/textView39"
                    android:textColorHint="@android:color/black"
                    android:textColor="@android:color/black"
                    android:layout_width="300.0dp"
                    android:layout_height="47.5dp"
                    android:inputType="textPersonName"
                    android:id="@+id/editNombreCuenta" />

            </TableRow>
            <TextView
                android:textAlignment="center"
                android:textColor="@android:color/holo_red_dark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:id="@+id/txtMensajeNombreCuenta" />

            <TableRow
                android:layout_width="287.0dp"
                android:layout_height="57.0dp"
                android:id="@+id/tableRow3">
                <ImageView
                    android:layout_width="37.0dp"
                    android:layout_height="49.0dp"
                    android:contentDescription=""
                    android:src="@drawable/tarjetas"
                    android:id="@+id/imageView5" />
                <Spinner
                    android:id="@+id/spinnercuenta"
                    android:layout_width="fill_parent"
                    android:layout_height="45.0dp"
                    android:prompt="@string/planet_prompt"/>
            </TableRow>
            <TextView
                android:textAlignment="center"
                android:textColor="@android:color/holo_red_dark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:id="@+id/txtMensajeTipoCuenta" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:id="@+id/tableRow5">

                <ImageView
                    android:layout_width="37.0dp"
                    android:layout_height="40dp"
                    android:contentDescription=""
                    android:src="@drawable/monto"
                    android:id="@+id/imageView6" />

                <EditText
                    android:hint="@string/textView27"
                    android:textColorHint="@android:color/black"
                    android:textColor="@android:color/black"
                    android:layout_width="300.0dp"
                    android:layout_height="wrap_content"
                    android:inputType="phone"
                    android:id="@+id/editMonto" />
            </TableRow>
            <TextView
                android:textAlignment="center"
                android:textColor="@android:color/holo_red_dark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:id="@+id/textMensajeMonto" />

            <Button
                android:background="@color/background4"
                android:textColor="@android:color/black"
                android:layout_width="125.5dp"
                android:layout_height="50.0dp"
                android:text="@string/btnFecha"
                android:id="@+id/btnFecha" />

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="50.0dp"
                android:id="@+id/tableRow8">

                <ImageView
                    android:layout_width="37.0dp"
                    android:layout_height="52.0dp"
                    android:contentDescription=""
                    android:src="@drawable/calendario"
                    android:id="@+id/imageView4" />

                <EditText
                    android:hint=""
                    android:textColorHint="@android:color/black"
                    android:textColor="@android:color/black"
                    android:layout_width="300.0dp"
                    android:layout_height="57.5dp"
                    android:inputType="date"
                    android:id="@+id/editFecha" />
            </TableRow>

            <TextView
                android:textAlignment="center"
                android:textColor="@android:color/holo_red_dark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:id="@+id/textMensajeFecha" />
            <Button
                android:layout_gravity="center"
                android:background="@color/background8"
                android:textColor="@android:color/black"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/btnGuardarIngreso"
                android:id="@+id/btnGuardarIngreso" />
            <TextView
                android:textAlignment="center"
                android:textColor="@android:color/holo_red_dark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:id="@+id/txtMensaje" />
            <Button
                android:layout_gravity="center"
                android:background="@color/background8"
                android:textColor="@android:color/black"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/btnCancelar"
                android:id="@+id/btnCancelar" />
            <TextView
                android:textAlignment="center"
                android:textColor="@android:color/holo_red_dark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:id="@+id/textView8" />
            <TextView
                android:textAlignment="center"
                android:textColor="@android:color/holo_red_dark"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:id="@+id/textView9" />


        </TableLayout>
    </ScrollView>
</LinearLayout>
